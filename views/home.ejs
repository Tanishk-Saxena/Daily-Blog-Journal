<%- include("partials/header") -%>

<h1>Home</h1>
<p><%=homeStartingContent%></p>
<% entries.forEach(function(entry){ %>
  <h2><%=entry.blogTitle%></h2>
  <p>
    <%=entry.blogPost.substring(0, 100) + "..."%>
    <a href="/posts/<%=entry._id%>">Read More</a>
  </p>
<% }); %>

<%- include("partials/footer") -%>
